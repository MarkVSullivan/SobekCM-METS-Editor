<?xml version="1.0" encoding="utf-8" ?>

<!-- Begin the XML for this input template -->
<input_template
  xmlns="http://digital.uflib.ufl.edu/metadata/sobekcm_template/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://digital.uflib.ufl.edu/metadata/sobekcm_template/
		http://digital.uflib.ufl.edu/metadata/sobekcm_template/sobekcm_template.xsd">

  <!-- Define the information about this input template -->
  <name>Electronic Theses and Dissertations Add-On</name>
  <name language="spa">Plantilla ETD</name>
  <name language="fre">Calibre ETD</name>
  <notes>This contains information specific to Electronic Theses and Dissertations, such as committee members and degree information.</notes>
  <dateCreated>May 19, 2011</dateCreated>
  <lastModified>May 19, 2011</lastModified>
  <creator>Mark V Sullivan</creator>
  <includeUserAsAuthor>false</includeUserAsAuthor>
  <uploads>FILE_OR_URL</uploads>
  <uploadMandatory>false</uploadMandatory>

  <!-- This defines the inputs which are available for the user -->
  <inputs>
    <page>
      <name language="eng">ETD</name>
      <name language="fre">ETD</name>
      <name language="spa">ETD</name>
      <panel>
        <name language="eng">Committee Information</name>
        <element type="ETD_CommitteeChair" />
        <element type="ETD_CommitteeCoChair" />
        <element type="ETD_CommitteeMember" repeatable="true" />
      </panel>
      <panel>
        <name>Degree Information</name>
        <element type="ETD_Degree" />
        <element type="ETD_DegreeDiscipline" />
        <element type="ETD_DegreeGrantor" />
        <element type="ETD_DegreeLevel" />
        <element type="ETD_GraduationDate" />
      </panel>
    </page>
  </inputs>
</input_template>
<!-- End of input template XML -->