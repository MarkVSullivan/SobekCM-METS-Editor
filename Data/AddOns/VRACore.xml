<?xml version="1.0" encoding="utf-8" ?>

<!-- Begin the XML for this input template -->
<input_template
  xmlns="http://digital.uflib.ufl.edu/metadata/sobekcm_template/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://digital.uflib.ufl.edu/metadata/sobekcm_template/
		http://digital.uflib.ufl.edu/metadata/sobekcm_template/sobekcm_template.xsd">

  <!-- Define the information about this input template -->
  <name>VRA Core Add-On</name>
  <name language="spa">Plantilla SobekCM</name>
  <name language="fre">Calibre SobekCM</name>
  <notes>This add-on includes access to several VRACore elements for describing visual materials, especially in use for museum artifacts.  These fields are encoded as a MODS extension schema.</notes>
  <dateCreated>May 19, 2011</dateCreated>
  <lastModified>May 19, 2011</lastModified>
  <creator>Mark V Sullivan</creator>
  <includeUserAsAuthor>false</includeUserAsAuthor>
  <uploads>FILE_OR_URL</uploads>
  <uploadMandatory>false</uploadMandatory>

  <!-- This defines the inputs which are available for the user -->
  <inputs>
    <page>
      <name language="eng">VRACore</name>
      <name language="fre">VRACore</name>
      <name language="spa">VRACore</name>
      <panel>
        <name language="eng">Description of Works of Visual Culture</name>
        <name language="spa">Description of Works of Visual Culture</name>
        <name language="fre">Description of Works of Visual Culture</name>
        <element type="VRA_CulturalContext" />
        <element type="VRA_Inscription" />
        <element type="VRA_Materials" />
        <element type="VRA_Measurements" />
        <element type="VRA_StateEdition" />
        <element type="VRA_StylePeriod" />
        <element type="VRA_Technique" />
      </panel>
    </page>
  </inputs>
</input_template>
<!-- End of input template XML -->