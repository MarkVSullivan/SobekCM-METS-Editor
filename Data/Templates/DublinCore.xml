<?xml version="1.0" encoding="utf-8" ?>

<!-- Begin the XML for this input template -->
<input_template
  xmlns="http://digital.uflib.ufl.edu/metadata/sobekcm_template/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://digital.uflib.ufl.edu/metadata/sobekcm_template/
		http://digital.uflib.ufl.edu/metadata/sobekcm_template/sobekcm_template.xsd">


	<!-- Define the information about this input template -->
	<name>Dublin Core Metadata Template</name>
	<name language="spa">Plantilla Dublin Core</name>
	<name language="fre">Calibre Dublin Core</name>
	<notes>Basic Dublin core template</notes>
	<dateCreated>May 19, 2011</dateCreated>
	<lastModified>May 19, 2011</lastModified>
	<creator>Mark V Sullivan</creator>
	<includeUserAsAuthor>false</includeUserAsAuthor>
	<uploads>FILE_OR_URL</uploads>
	<uploadMandatory>false</uploadMandatory>

	<!-- This defines the inputs which are available for the user -->
	<inputs>
		<page>
			<name language="eng">Material Information</name>
			<name language="fre">Matériel d'information</name>
			<name language="spa">Información de Materiales</name>
			<panel>
				<name language="eng">Bibliographic Information</name>
				<name language="fre">D'information Bibliographique</name>
				<name language="spa">Información Bibliográfica</name>
				<element type="Title" subtype="simple" mandatory="true" />
				<element type="OtherTitle" subtype="simple" />
				<element type="Creator" subtype="simple" repeatable="true" />
				<element type="Contributor" subtype="simple" repeatable="true" />
				<element type="Spatial" subtype="dublincore" repeatable="true" />
				<element type="Date" subtype="dublincore" />
				<element type="Note" subtype="dublincore" repeatable="true" />
				<element type="Format" subtype="dublincore" />
				<element type="Identifier" subtype="simple" repeatable="true" />
				<element type="Language" repeatable="true">
					<element_data>
						<source_xml>\Data\languages3way.xml</source_xml>
					</element_data>
				</element>
				<element type="Publisher" subtype="simple" repeatable="true" />
				<element type="RelatedItem" subtype="dublincore" repeatable="true" />
				<element type="Rights" />
        <element type="SourceNOte" subtype="dublincore" repeatable="true" />
				<element type="Subject" subtype="dublincore" repeatable="true" />
				<element type="Type" subtype="uncontrolled" mandatory="true" repeatable="false" />
			</panel>
		</page>
		<page>
			<name>METS Information</name>
			<panel>
				<name>METS Header Information</name>
				<element type="RecordStatus" />
				<element type="METS_ObjectID" />
				<element type="Source" subtype="complex" />
			</panel>
		</page>
	</inputs>
</input_template>
<!-- End of input template XML -->