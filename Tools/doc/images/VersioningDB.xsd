<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="VersionChecker" targetNamespace="DLC" xmlns:mstns="DLC" xmlns="DLC" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="VersionChecker" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Application">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Name" type="xs:string" minOccurs="0" />
              <xs:element name="NewVersion" type="xs:string" minOccurs="0" />
              <xs:element name="LastOldVersion" type="xs:string" minOccurs="0" />
              <xs:element name="SetupFile" type="xs:string" minOccurs="0" />
              <xs:element name="Mandatory" type="xs:boolean" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>