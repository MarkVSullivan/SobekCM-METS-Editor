<?xml version="1.0" encoding="utf-8" ?>

<!-- Begin the XML for this input template -->
<input_template
  xmlns="http://digital.uflib.ufl.edu/metadata/sobekcm_template/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://digital.uflib.ufl.edu/metadata/sobekcm_template/
		http://digital.uflib.ufl.edu/metadata/sobekcm_template/sobekcm_template.xsd">

  <!-- Define the information about this input template -->
  <name>DarwinCore Add-On</name>
  <name language="spa">Plantilla SobekCM</name>
  <name language="fre">Calibre SobekCM</name>
  <notes>This add-on provides support for zoological taxonomies for species information, such as kingdom, phylum, species, common name, etc..</notes>
  <dateCreated>May 19, 2011</dateCreated>
  <lastModified>May 19, 2011</lastModified>
  <creator>Mark V Sullivan</creator>
  <includeUserAsAuthor>false</includeUserAsAuthor>
  <uploads>FILE_OR_URL</uploads>
  <uploadMandatory>false</uploadMandatory>

  <!-- This defines the inputs which are available for the user -->
  <inputs>
    <page>
      <name language="eng">DarwinCore</name>
      <name language="fre">DarwinCore</name>
      <name language="spa">DarwinCore</name>
      <panel>
        <name>Scientific Taxonomy</name>
        <element type="ZT_Kingdom" />
        <element type="ZT_Phylum" />
        <element type="ZT_Order" />
        <element type="ZT_Family" />
        <element type="ZT_Genus" />
        <element type="ZT_Class" />
        <element type="ZT_ScientificEpithet" />
        <element type="ZT_ScientificName" />
        <element type="ZT_TaxonRank" />
        <element type="ZT_CommonName" />
      </panel>
    </page>
  </inputs>
</input_template>
<!-- End of input template XML -->